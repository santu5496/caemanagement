# Environment Configuration Template
# Copy this file to .env and update with your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/friendscars_db

# Session Security Key (Change this to a random secret key)
SESSION_SECRET=your-super-secret-key-here-change-me

# Flask Environment
FLASK_ENV=production

# Upload Settings (Optional - defaults work fine)
UPLOAD_FOLDER=static/uploads
MAX_CONTENT_LENGTH=16777216

# Admin Credentials (Default: username=abc, password=123)
# These are set in the database, not environment variables